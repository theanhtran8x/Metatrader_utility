{% extends 'base-uboot.jinja2' %}

{% set console_device = 'ttySC0' %}
{% set baud_rate = '115200' %}
{% set device_type = "r8a7743-iwg20d-q7" %}
{% set bootloader_prompt = bootloader_prompt|default('iWave-G20M >') %}
{% set kernel_file = kernel_file|default('uImage') %}
{% set dtb_file = dtb_file|default('r8a7743-iwg20d-q7.dtb') %}
{% set bootm_kernel_addr = '0x40007fc0' %}
{% set bootm_ramdisk_addr = '0x41f00000' %}
{% set bootm_dtb_addr = '0x48000000' %}
{% set uboot_mkimage_arch = 'arm' %}
{% set boot_character_delay = 20 %}
{% set test_character_delay = 20 %}
{% set storage_info = [{'MMC': '/dev/mmcblk1p1'}] %}
{% set uboot_error_messages = [
  'Resetting CPU',
  'Must RESET board to recover',
  'Retry time exceeded; starting again',
  'ERROR: The remote end did not respond in time.',
  'File not found',
  'Bad Linux ARM64 Image magic!',
  'Wrong Ramdisk Image Format',
  'Ramdisk image is corrupt or invalid',
  'ERROR: Failed to allocate',
  'TFTP error: trying to overwrite reserved memory'
  'ether_avb: alloc failed'
] %}
